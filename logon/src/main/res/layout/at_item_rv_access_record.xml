<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/rl_content"
    android:layout_width="match_parent"
    android:layout_height="252px"
    android:layout_marginLeft="45px"
    android:layout_marginRight="45px"
    android:layout_marginBottom="30px"
    android:background="@drawable/selector_12px_f5f5f5_eeeeee"
    android:paddingLeft="45px">

    <TextView
        android:id="@+id/tv_park_lot"
        android:layout_width="600px"
        android:layout_height="wrap_content"
        android:ellipsize="end"
        android:maxLines="1"
        android:layout_marginTop="30px"
        android:textColor="@color/_999999"
        android:textSize="39px" />

    <TextView
        android:id="@+id/tv_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_margin="33px"
        android:textColor="@color/_666666"
        android:textSize="36px" />

    <TextView
        android:id="@+id/tv_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="126px"
        android:textColor="@color/_333333"
        android:textSize="48px" />

    <TextView
        android:id="@+id/tv_in_out"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_marginRight="66px"
        android:layout_marginTop="126px"
        android:textColor="@color/_86523C"
        android:textSize="48px" />
</RelativeLayout>